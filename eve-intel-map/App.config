      <setting name="colorKos0" serializeAs="String">
        <value>224, 255, 224</value>
      </setting>
      <setting name="colorKos1" serializeAs="String">
        <value>255, 224, 224</value>
      </setting>
      <setting name="colorKos3" serializeAs="String">
