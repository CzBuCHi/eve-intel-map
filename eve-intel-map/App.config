  </startup>

  <connectionStrings>
    <add name="eve_intel_map.Properties.Settings.eve_intel" connectionString="data source=D:\github\eve-intel\eve-intel-map\eve-intel.sqlite"
      providerName="System.Data.SQLite.EF6" />
  </connectionStrings>

  <system.data>
    <DbProviderFactories>
      <remove invariant="System.Data.SQLite" />
