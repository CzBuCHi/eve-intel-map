<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="eve_intel_server.Domain" assembly="eve-intel-server">

  <class name="EveMapRegion" table="EveMapRegions">
    <id name="Id" />
    <property name="Name" />
  </class>

  <class name="EveMapSolarsystem" table="EveMapSolarsystems">
    <id name="Id" />
    <property name="Name" />
    <many-to-one name="Region" class="EveMapRegion" />
  </class>

  <class name="EveMapSolarsystemJump" table="EveMapSolarsystemJumps">
    <composite-id>
      <key-many-to-one name="FromSolarsystem" class="EveMapSolarsystem" />
      <key-many-to-one name="ToSolarsystem" class="EveMapSolarsystem" />
    </composite-id>
  </class>

  <class name="EveRace" table="EveRaces">
    <id name="Id" />
    <property name="Name" />
  </class>

  <class name="EveShipType" table="EveShipTypes">
    <id name="Id" />
    <property name="Name" />
  </class>

  <class name="EveShipInfo" table="EveShipInfos">
    <id name="Id" />
    <property name="ShipName" />
    <many-to-one name="ShipType" class="EveShipType" />
    <property name="TechLevel" />
    <many-to-one name="Race" class="EveRace" />
  </class>

  <class name="CvaCharacterInfo" table="CvaCharacterInfos">
    <id name="Id" />
    <property name="Type" />
    <property name="Label" />
    <property name="Icon" />
    <property name="Kos" />
    <property name="EveId" />
    <many-to-one name="Corp" class="CvaCorporationInfo" />
  </class>

  <class name="CvaCorporationInfo" table="CvaCorporationInfos">
    <id name="Id" />
    <property name="Type" />
    <property name="Label" />
    <property name="Icon" />
    <property name="Kos" />
    <property name="EveId" />
    <property name="Ticker" />
    <property name="Npc" />
    <many-to-one name="Alliance" class="CvaAllianceInfo" />
  </class>

  <class name="CvaAllianceInfo" table="CvaAllianceInfos">
    <id name="Id" />
    <property name="Type" />
    <property name="Label" />
    <property name="Icon" />
    <property name="Kos" />
    <property name="EveId" />
    <property name="Ticker" />
  </class>

  <class name="IntelData" table="IntelData">
    <id name="Id" />
    <one-to-one name="Character" class="CvaCharacterInfo" />
    <many-to-one name="ShipInfo" class="EveShipInfo" />
    <property name="ShipInfoDate" />
    <property name="ShipInfoConfirmed" />
    <many-to-one name="Solarsystem" class="EveMapSolarsystem" />
    <property name="SolarsystemDate" />
  </class>

</hibernate-mapping>